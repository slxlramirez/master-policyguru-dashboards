    <panel>
      <single>
        <title>Allowed Calls (vs Previous), Trend 8</title>
        <search>
          <query>| dbxquery 
connection="mssv_db" 
timeout=1800 query="
SELECT 
       to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss') AS weekly,
       count(guid) as calls
FROM   pg_fs_pfd_cdr
WHERE  datekey &gt;= CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - (7*8) AND
       datekey  &lt; CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) AND
       policydisposition = 'ALLOWED'
GROUP BY
         to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss')
ORDER BY 
         to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss') DESC
LIMIT 8
" shortnames=t
| table weekly calls
| eval _time=strptime(weekly,"%Y-%m-%d %H:%M:%S")
| timechart fixedrange=true span=1w sum(calls)
| sort _time</query>
          <earliest>-8w@w</earliest>
          <latest>@w</latest>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x65a637","0x6db7c6","0xf7bc38","0xf58f39","0xf7bc38","0xd93f3c"]</option>
        <option name="rangeValues">[0,30,70,100,5000]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendDisplayMode">percent</option>
        <option name="trendInterval">-7d</option>
        <option name="useColors">1</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Terminated Calls (vs Previous), Trend 8</title>
        <search>
          <query>| dbxquery 
connection="mssv_db" 
timeout=1800 query="
SELECT 
       to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss') AS weekly,
       count(guid) as calls
FROM   pg_fs_pfd_cdr
WHERE  datekey &gt;= CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - (7*8) AND
       datekey  &lt; CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) AND
       policydisposition = 'TERMINATED'
GROUP BY
         to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss')
ORDER BY 
         to_char(date_trunc('week', datekey + interval '1 day'),'YYYY-MM-DD hh24:mi:ss') DESC
LIMIT 8
" shortnames=t
| table weekly calls
| eval _time=strptime(weekly,"%Y-%m-%d %H:%M:%S")
| timechart fixedrange=true span=1w sum(calls)
| sort _time</query>
          <earliest>-8w@w</earliest>
          <latest>@w</latest>
        </search>
        <option name="drilldown">none</option>
