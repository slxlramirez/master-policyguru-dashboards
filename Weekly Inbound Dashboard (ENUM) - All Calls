  <row>
    <panel>
      <title>Suspicious Callers | Spikey</title>
      <table>
        <title>Spikey Callers (excl Unan Dests)</title>
        <search>
          <query>| dbxquery 
connection="mssv_db" 
timeout=1800 query="
SELECT b.*
FROM
(
SELECT 
   CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 as week_of_year,
   sourcenumber,
   sum(calls) as total_calls,
   max(calls) as max_calls,
   ROUND((max(calls)::numeric/sum(calls))*100,1) as perc_from_max_day
FROM
(
SELECT  
        TO_CHAR (a.flowtime_t,'YYYY-MM-DD') as flowtime_ts_gmt,
        sourcenumber,
        COUNT(a.guid) as calls
FROM    pg_fs_pfd_cdr a
WHERE   datekey &gt;= (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7) AND
            datekey  &lt; (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int)    ) AND
            sourcenumber IS NOT NULL AND
            routername LIKE 'enum%' AND
       sourcenumber NOT IN 
       (
       SELECT distinct value_original
       FROM   pg_directory
       WHERE  list_name IN ('Graylist_PN','Whitelist_PN')
       ) AND
       policydisposition = 'ALLOWED' AND
       destinationnumber NOT IN 
       (
       SELECT
       a.destinationnumber
       FROM
       (
       SELECT
        destinationnumber,
        count(guid) as not_answered_calls
       FROM    pg_fs_fe_cdr a
       WHERE   datekey &gt;= (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7) AND
               datekey  &lt; (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int)    ) AND
               direction = 'INBOUND' AND
               a.calldispositions NOT IN ('ANSWERED','ANSWERED,UNKNOWN')
       GROUP BY destinationnumber
       ORDER BY destinationnumber
       ) a LEFT OUTER JOIN 
       (
       SELECT
        destinationnumber,
        count(guid) as answered_calls
       FROM    pg_fs_fe_cdr a
       WHERE   datekey &gt;= (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7) AND
               datekey  &lt; (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int)    ) AND
               direction = 'INBOUND' AND
               a.calldispositions IN ('ANSWERED','ANSWERED,UNKNOWN')
       GROUP BY destinationnumber
       ORDER BY destinationnumber
       ) b ON a.destinationnumber=b.destinationnumber
       WHERE b.answered_calls IS NULL
              )
       GROUP BY  TO_CHAR (a.flowtime_t,'YYYY-MM-DD'), sourcenumber
       ORDER BY  TO_CHAR (a.flowtime_t,'YYYY-MM-DD'), COUNT(a.guid)
       ) a
       GROUP BY sourcenumber
       ) b 
WHERE total_calls &gt;= 10 AND
      perc_from_max_day &gt; .5
ORDER BY perc_from_max_day DESC, total_calls DESC
LIMIT 10
" shortnames=t
| table week_of_year sourcenumber total_calls max_calls perc_from_max_day
| rename week_of_year as "Reported Week" sourcenumber as "Source Number" total_calls as "Total" max_calls as "Max" perc_from_max_day as "% Max Day"</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">cell</option>
        <format type="number" field="perc_from_max_day">
          <option name="precision">1</option>
          <option name="unit">%</option>
        </format>
        <format type="color" field="% Max Day">
          <colorPalette type="minMidMax" maxColor="#D6563C" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="number" field="% Max Day">
          <option name="precision">1</option>
          <option name="unit">%</option>
        </format>
        <drilldown>
          <link target="_blank">/app/search/data_analyis_spikey_source_last_week_aggregated_direct__no_manual_run?sourceNumber_token=$click.value2$</link>
        </drilldown>
      </table>
    </panel>
    <panel>
      <title>Suspicious Callers | Spread</title>
      <table>
        <title>High Spread Callers</title>
        <search>
          <query>| dbxquery
connection="mssv_db" 
query="
SELECT week_of_year,
       sourcenumber,
       total_calls,
       spread,
       ROUND(spread::numeric/total_calls,3)*100 as perc_spread
FROM 
(
    SELECT  
            CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 as week_of_year,
            sourcenumber,
            count(a.guid) as total_calls,
            count(distinct a.destinationnumber) as SPREAD
    FROM    pg_fs_pfd_cdr a
    WHERE   datekey &gt;= (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7) AND
            datekey  &lt; (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int)    ) AND
            sourcenumber IS NOT NULL AND
            routername LIKE 'enum%' AND
       sourcenumber NOT IN 
       (
       SELECT distinct value_original
       FROM   pg_directory
       WHERE  list_name IN ('Graylist_PN','Whitelist_PN')
       ) AND
       policydisposition = 'ALLOWED' AND
       lower(sourcenumber) NOT IN ('anonymous')
    GROUP BY sourcenumber
) a
ORDER BY spread DESC
LIMIT 10" 
shortnames=t 
| table week_of_year sourcenumber total_calls spread perc_spread
| rename week_of_year as "Reported Week", sourcenumber as "Source Number", total_calls AS "Total Calls", spread AS "Destinations Called" perc_spread as "% Spread"</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">cell</option>
        <format type="number" field="Destinations Called">
          <option name="precision">0</option>
        </format>
        <format type="color" field="% Spread">
          <colorPalette type="minMidMax" maxColor="#D6563C" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="number" field="% Spread">
          <option name="precision">1</option>
          <option name="unit">%</option>
        </format>
        <drilldown>
          <link target="_blank">/app/search/data_analyis_inbound_source_last_week_aggregated_direct__no_manual_run?sourceNumber_token=$click.value2$</link>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Suspicious Callers | BU Spread</title>
      <table>
        <title>by Business Unit Spread</title>
        <search>
          <query>| dbxquery
connection="mssv_db" 
query="
SELECT x.sourcenumber,
       sum(total_calls) as calls,
       count(distinct destinationnumber) as spread,
       count(distinct groups) as bu_spread
FROM
(
SELECT a.sourcenumber, a.destinationnumber, a.total_calls, b.groups
FROM
(
SELECT      sourcenumber,
            destinationnumber,
            count(a.guid) as total_calls
FROM        pg_fs_pfd_cdr a
WHERE       datekey &gt;= (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7) AND
            datekey  &lt; (CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int)    ) AND
            sourcenumber IS NOT NULL AND
       sourcenumber NOT IN 
       (
       SELECT distinct value_original
       FROM   pg_directory
       WHERE  list_name IN ('Graylist_PN','Whitelist_PN')
       ) AND
       policydisposition = 'ALLOWED'
GROUP BY sourcenumber, destinationnumber
) a LEFT OUTER JOIN
(
 SELECT t.value_original,
        array_to_string(array_agg(list_name), ',') as groups
 FROM
 (
 SELECT list_name, value_original
 FROM   pg_directory 
 WHERE  list_name LIKE '%BU%'
 ORDER BY list_name, value_original
 ) t
 GROUP BY value_original
) b ON a.destinationnumber=b.value_original
) x 
WHERE sourcenumber NOT IN ('Unavailable','Anonymous','unknown','anonymous')
GROUP BY sourcenumber
--ORDER BY sourcenumber
ORDER BY count(distinct groups) DESC, sum(total_calls) DESC
LIMIT 10
" 
shortnames=t
| table sourcenumber calls spread bu_spread</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
      </table>
    </panel>
    <panel>
      <title>Suspicious Callers | Invalid Format</title>
      <table>
        <title>Invalid Number Callers</title>
        <search>
          <query>| dbxquery 
maxrows=250000
connection="mssv_db" 
timeout=1800 query="
SELECT
        CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 as week_of_year,
        regexp_matches(sourcenumber, '^\+1[0-9]{3}[0-1]{1}[0-9]{6}') as sourcenumber_regexp,
        sourcenumber,
        COUNT(a.guid) as calls
FROM   pg_fs_pfd_cdr a
WHERE  datekey &gt;= CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 AND
       datekey  &lt; CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) AND
       direction = 'INBOUND' AND
       policydisposition = 'ALLOWED' AND
       sourcenumber NOT IN 
       (
       SELECT distinct value_original
       FROM   pg_directory
       WHERE  list_name IN ('Graylist_PN','Whitelist_PN')
       )
GROUP BY regexp_matches(sourcenumber, '^\+1[0-9]{3}[0-1]{1}[0-9]{6}'),
         sourcenumber
 ORDER BY count(a.guid) DESC
" shortnames=t
| append
[
| dbxquery 
maxrows=250000
connection="mssv_db" 
timeout=1800 query="
SELECT
        CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 as week_of_year,
        regexp_matches(sourcenumber, '^\+1([0-9]{0,9})$$') as sourcenumber_regexp,
        sourcenumber,
        COUNT(a.guid) as calls
FROM   pg_fs_pfd_cdr a
WHERE  datekey &gt;= CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 AND
       datekey  &lt; CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) AND
       direction = 'INBOUND' AND
       policydisposition = 'ALLOWED'
GROUP BY regexp_matches(sourcenumber, '^\+1([0-9]{0,9})$$'),
         sourcenumber
 ORDER BY count(a.guid) DESC
" shortnames=t
]
| append
[
| dbxquery 
maxrows=250000
connection="mssv_db" 
timeout=1800 query="
SELECT
        CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 as week_of_year,
        regexp_matches(sourcenumber, '^\+1([01][0-9][0-9]|200)[0-9]{7}$$') as sourcenumber_regexp,
        sourcenumber,
        COUNT(a.guid) as calls
FROM   pg_fs_pfd_cdr a
WHERE  datekey &gt;= CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) - 7 AND
       datekey  &lt; CURRENT_DATE - CAST(EXTRACT(DOW FROM CURRENT_DATE) as int) AND
       direction = 'INBOUND' AND
       policydisposition = 'ALLOWED'
GROUP BY regexp_matches(sourcenumber, '^\+1([01][0-9][0-9]|200)[0-9]{7}$$'),
         sourcenumber
 ORDER BY count(a.guid) DESC
" shortnames=t
]
| table week_of_year sourcenumber calls
| sort -calls
| dedup sourcenumber
| head 10
| rename sourcenumber as "Source Number" calls as "Calls" week_of_year as "Reported Week"</query>
          <earliest>-15m</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
